{
  "name": "kiss4-web-client",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "npm run sy-pre-test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sy-pre-test": "node hooks/pre-test.js",
    "sy-pre-start": "node hooks/pre-start.js",
    "sy-pre-build": "node hooks/pre-build.js",
    "sy-post-build": "node hooks/post-build.js",
    "sy-ngw-copy": "node hooks/ngw-copy.js",
    "ngsw-config": "node_modules/.bin/ngsw-config dist ngsw-config.json",
    "sw": "npm run ngsw-config & npm run sy-ngw-copy",
    "sy-build": "npm run sy-pre-build & node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot --vendor-chunk --ngsw-config-path ngsw-config.json & npm run sy-post-build & npm run sw",
    "kiss:dev": "npm run sy-pre-start & node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json --aot --port=4300",
    "kiss:hmr": "npm run sy-pre-start & ng serve --hmr --aot --proxy-config proxy.conf.json --port=4300",
    "kiss:build": "npm run sy-build"
  },
  "private": true,
