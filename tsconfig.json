{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": "./src",
    "paths": {
      "@auth/*": [
        "./auth/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@node_modules/*": [
        "../node_modules/*"
      ],
      "globalize": [
        "../node_modules/globalize/dist/globalize"
      ],
      "globalize/*": [
        "../node_modules/globalize/dist/globalize/*"
      ],
      "cldr": [
        "../node_modules/cldrjs/dist/cldr"
      ],
      "cldr/*": [
        "../node_modules/cldrjs/dist/cldr/*"
      ],
      "jszip/*": [
        "node_modules/jszip/dist/jszip.min.js"
      ]
    },
    "module": "es2015"
  },
  "exclude": [
    "node_modules"
  ]
}